node('maven')
{
    stage('Git')
    {
        git 'https://github.com/spring-projects/spring-petclinic.git'
    }
    stage('Build')
    {
    sh 'pwd'
    sh 'whoami'
    sh 'mvn package' 
    }
    
    stage('Archive')
    {
    archiveArtifacts 'target/*.war'
    junit 'target/surefire-reports/*.xml'
    }
}

node('jfrog')
{
        stage('Jfrog')
        {
        sh 'pwd'
        sh 'whoami'
        sh 'docker --version'
        sh 'docker version'
        }
}
